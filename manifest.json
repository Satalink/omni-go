{
  "manifest_version": 3,
  "name": "Omni Go",
  "description": "Omni GO Redirects. Type 'go' and a keyword into the address bar to open url.",
  "version": "1.2024.0416.0816",
  "options_page": "src/options/options.html",
  "background": {
    "service_worker": "src/background/background.js",
    "type": "module"
  },
  "minimum_chrome_version": "92",
  "permissions": ["storage", "tabs"],
  "omnibox": { "keyword": "go" },
  "action": {
    "default_icon": {
      "16": "assets/extension-icons/icon16.png",
      "32": "assets/extension-icons/icon32.png"
    }
  },
  "icons": {
    "16": "assets/extension-icons/icon16.png",
    "32": "assets/extension-icons/icon32.png",
    "48": "assets/extension-icons/icon48.png",
    "128": "assets/extension-icons/icon.png"
  }
}
